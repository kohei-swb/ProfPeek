{
  "name": "UBCProfPeek",
  "description": "Displaying proffesor rate on workday",
  "version": "1.0",
  "manifest_version": 3,
  "permission":["scripting"],
  "host_permission": ["<all_urls>"],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "unofficialUBCProfPeek.png"
  },
  "content_scripts": [
    {
      "matches": ["https://wd10.myworkday.com/ubc/d/task/*"],
      "js": ["content.js"]
    }
  ]
}